{
    "name": "agashe/sigmaphp-db",
    "description": "PHP Database Tools",
	"keywords": ["SigmaPHP", "framework"],
    "type": "library",
	"license": "MIT",
    "autoload": {
        "psr-4": {
            "SigmaPHP\\DB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SigmaPHP\\DB\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Yousef",
            "email": "engineer.mohamed.yossef@gmail.com"
        }
    ],
    "require": {
        "doctrine/inflector": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "cp src/Console/templates/database.php.dist database.php",
            "./vendor/bin/phpunit tests/",
            "rm database.php"
        ]
    },
    "bin": "bin/sigma-db"
}
